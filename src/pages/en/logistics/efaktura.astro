---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import FeatureGuide from '../../../components/FeatureGuide.astro';
import { t } from '../../../i18n/utils';
const locale = 'en';
const feature = 'efaktura';
---
<BaseLayout title={t(locale, `seo.logistics.guides.${feature}.title`)} description={t(locale, `seo.logistics.guides.${feature}.description`)} locale={locale}>
  <FeatureGuide locale={locale} feature={feature} />
</BaseLayout>
